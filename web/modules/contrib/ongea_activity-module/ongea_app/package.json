    "webpack": "^4.14.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "serve": "react-scripts start",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "sh -ac '. .env.production';npm-run-all build-css build-js build:rename",
    "build:staging": "cp ./.env.staging ./.env.production && npm run build-css && npm run build-js",
    "build:rename": "cd build && cp static/js/*.js static/js/main.js && cp static/css/*.css static/css/main.css",
    "DELETE__build-rename": "npm-run-all build-rename-js build-rename-css build-fix-sourcemap",
    "DELETE__build-rename-js": "renamer --regex --find 'main.*.js' --replace 'main.js' 'build/static/js/*'",
    "DELETE__build-rename-css": "renamer --regex --find 'main.*.css' --replace 'main.css' 'build/static/css/*'",
    "DELETE__build-fix-sourcemap": "npm-run-all build-fix-sourcemap-js build-fix-sourcemap-css",
    "DELETE__build-fix-sourcemap-js": "replace \"# sourceMappingURL=main..*.js.map\" \"# sourceMappingURL=main.js.map\" build\\static\\js\\main.js",
    "DELETE__build-fix-sourcemap-css": "replace \"# sourceMappingURL=main..*.map\" \"# sourceMappingURL=main.css.map\" build\\static\\css\\main.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
